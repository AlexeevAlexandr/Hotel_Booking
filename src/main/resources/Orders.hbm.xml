<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dataBaseConnect.Orders" table="orders">
        <id name="dateRegistration" column="dateregistration" type="int">
            <generator class="native"/>
        </id>
        <property name="number" type="int" column="number"/>
        <property name="dateFrom" column="datefrom" type="date"/>
        <property name="dateTill" column="datetill" type="date"/>
        <property name="name" column="name" type="string"/>
        <property name="cost" column="cost" type="int"/>
    </class>
</hibernate-mapping>